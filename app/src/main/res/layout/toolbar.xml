        ningbaoqi:rightbackground="@mipmap/ic_launcher_round"
        ningbaoqi:righttext="@string/more"
        ningbaoqi:righttextcolor="#ffffff"
        ningbaoqi:title="zidingyibiaoqian"
        ningbaoqi:titlecolor="#123456"
